      <%if(role==1){ %>
      <div class="col-md-3 launch-icon-panel">
      <section ng-controller="NotificationShowCtrl" ng-cloak>
        <notification-icon animation="grow" count="notificationCount['0'].num" ng-if="notificationCount['0'].userID==getID.id">
          <a href="/feed/show?auth=<%= auth %>">
          <i class="fa fa-bell-o fa-5x"></i></a>
        </notification-icon>
        <label>Notification</label>
      </section>
        </div>
      <div class="col-md-3 launch-icon-panel">
            <a href="/usersauth?auth=<%= auth %>">
            <i class="fa fa-user-o fa-5x"></i></a>
