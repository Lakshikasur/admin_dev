      </div>
      <div class="col-md-3 launch-icon-panel">
      <a href="/student/init?auth=<%= auth %>">
      <i class="fa fa-graduation-cap fa-5x"></i></a>
        <label>Students</label>
      </div>
    <% } %>
      <%if(role==3){ %>
    <div class="col-md-3 launch-icon-panel">
      <section ng-controller="NotificationShowCtrl" ng-cloak>
