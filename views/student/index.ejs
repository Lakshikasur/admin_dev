        <div id="confirm" class="tab-pane fade ">
          <div class="col-md-6 col-md-offset-3  advance-search">
            <form>
              <div class="input-group input-group-lg">
                <input type="text" name="search" id="advance-search" class="form-control advance-search-box"
                       placeholder="Search Student Reference" aria-describedby="sizing-addon1">
                <div class="separator">
                  <div class="separator-boder"></div>
                </div>
                <span class="input-group-addon" name="searchRef" id="searchInActive"><i class="fa fa-eye-open"
                                                                                        aria-hidden="true"></i></span>
              </div>
            </form>
          </div>
          <div class="col-md-10 col-md-offset-1 well formpackage">

            <table id="studenttable" class="table table-bordred table-striped">

              <thead>

              <th>Email</th>
              <th>Confirm</th>
              </thead>
              <tbody id="studentdata">
              </tbody>

            </table>
          </div>
        </div>
        <div id="updatePackage" class="tab-pane fade ">
          <div class="col-md-12 well formpackage">
            <table id="studentActTable" class="table table-bordred table-striped">

              <thead>
              <th>Email </th>
              <th>Package Price</th>
              <th>Payment Method </th>
              <th>Update Package</th>

              </thead>
              <tbody id="updatePackageDetails">

              </tbody>

            </table>

          </div>
        </div>
      </div>
      <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
           aria-hidden="true" id="payConfirmModel">
        <div class="modal-dialog modal-lg">
          <div class="modal-content">
            <div class="modal-header">
              <h4 class="modal-title" id="myModalLabel">Insert Payment details</h4>
            </div>
            <div class="modal-body">
              <form name="Confirmform">
                <div class="row">
                  <div class="col-md-12">
                    <div class="row">

                      <div class="form-group col-md-6">
                        <label>Email</label>
                        <input type="text" id="m_ename" name="m_ename" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>
                      <div class="form-group col-md-6 editstpaymnetmodal" id="divamount">
                        <label>Amount<span class="glyphicon glyphicon-remove-circle editPaymentModel" style="color:#286090"></span></label>
                        <input type="text" id="m_amount" name="m_amount" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>

                    </div>
                    <div class="row">
                      <div class="form-group col-md-6 editstpaymnetmodal" id="divpay_method">
                        <label>Payment Method<span class="glyphicon glyphicon-remove-circle editPaymentModel" style="color:#286090"></span></label>
                        <input type="text" id="pa_type" name="pa_type" class="form-control" disabled>
                        <!--   <select id="pa_type" name="pa_type" class="form-control">
                             <option>Select Payment Method</option>
                             <option value="Dialog Ez-Cash">Dialog Ez-Cash</option>
                             <option value="Mobitel M-Cash">Mobitel M-Cash</option>
                             <option value="Direct Debit">Direct Debit</option>
                             <option value="Standing Order">Standing Order</option>

                           </select>-->

                        <div class="help-block"></div>
                      </div>
                      <div class="form-group col-md-6">
                        <label>Status</label>
                        <input type="text" id="st_name" name="st_name" value="success" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>

                      <input type="hidden" id="sttoken" name="sttoken">

                    </div>

                    <div class="row">
                      <div id="alertSt">
                      </div>
                    </div>

                    <%- partial('../layout/common/loader.ejs') %>
                  </div>
                </div>
                <input type='hidden' name='_csrf' value='<%= _csrf %>'>

              </form>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-primary" id="payconfirm">Active</button>
              <button type="button" class="btn btn-secondary" id="paycancel" data-dismiss="modal">Close</button>

            </div>

          </div>
        </div>
      </div>
      <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
           aria-hidden="true" id="update_student_package">
        <div class="modal-dialog modal-lg">
          <div class="modal-content">
            <div class="modal-header">
              <h4 class="modal-title" id="myModalLabel">Package Updation  details</h4>
            </div>
            <div class="modal-body">
              <form name="Confirmform">
                <div class="row">
                  <div class="col-md-12">
                    <div class="row">

                      <div class="form-group col-md-6">
                        <label>Email</label>
                        <input type="text" id="email" name="email" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>
                      <div class="form-group col-md-6 editstpaymnetmodal" id="divamount">
                        <label>Amount<span class="glyphicon glyphicon-remove-circle editPaymentModel" style="color:#286090"></span></label>
                        <input type="text" id="amount" name="paid_amount" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>
                      <input  name="signup_token" id="signup_token" type="hidden" >

                    </div>
                    <div class="row">
                      <div class="form-group col-md-6 editstpaymnetmodal" id="divpay_method">
                        <label>Payment Method<span class="glyphicon glyphicon-remove-circle editPaymentModel" style="color:#286090"></span></label>
                        <input type="text" id="payment_method" name="payment_method" class="form-control" disabled>
                        <!--   <select id="pa_type" name="pa_type" class="form-control">
                             <option>Select Payment Method</option>
                             <option value="Dialog Ez-Cash">Dialog Ez-Cash</option>
                             <option value="Mobitel M-Cash">Mobitel M-Cash</option>
                             <option value="Direct Debit">Direct Debit</option>
                             <option value="Standing Order">Standing Order</option>

                           </select>-->

                        <div class="help-block"></div>
                      </div>
                      <div class="form-group col-md-6">
                        <label>Status</label>
                        <input type="text" id="status" name="status" value="success" class="form-control" disabled>

                        <div class="help-block"></div>
                      </div>

                      <input type="hidden" id="sttoken" name="sttoken">

                    </div>

                    <div class="row">
                      <div id="alertSt">
                      </div>
                    </div>

                    <%- partial('../layout/common/loader.ejs') %>
                  </div>
                </div>
                <input type='hidden' name='_csrf' value='<%= _csrf %>'>

              </form>
            </div>
            <div id="responseText" style="float:center;"></div>
            <div class="modal-footer">
              <button type="button" class="btn btn-primary" id="upadte_pkg">Update Package</button>
              <button type="button" class="btn btn-secondary" id="paycancel" data-dismiss="modal">Close</button>
            </div>

          </div>
        </div>
