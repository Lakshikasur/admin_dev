      <div class="tab-content" >
        <div id="active" class="tab-pane fade in active">

          <div class="col-md-12 well formpackage">
            <table id="studentActTable" class="table table-bordred table-striped">

              <thead>
              <th>Email</th>
              <th>reference</th>
              <th>View</th>
              </thead>
              <tbody id="activeStudentData">

              </tbody>

            </table>

          </div>

        </div>
        <div id="nonactive" class="tab-pane fade">
          <div class="col-md-12  well formpackage">
            <table id="studentNoActTable" class="table table-bordred table-striped">

              <thead>

              <thead>
              <th>Email</th>
              <th>reference</th>
              <th>Active</th>
              </thead>
              <tbody id="inActivStudentData">

              </tbody>

            </table>
